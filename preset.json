{
  "useTaobaoRegistry": true,
  "packageManager": "yarn",
  "presets": {
    "basic": {
      "useConfigFiles": true,
      "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-pwa": {},
        "@vue/cli-plugin-router": {
          "historyMode": true
        },
        "@vue/cli-plugin-vuex": {},
        "@vue/cli-plugin-eslint": {
          "config": "prettier",
          "lintOn": ["save"]
        }
      },
      "vueVersion": "2",
      "cssPreprocessor": "node-sass"
    }
  }
}
